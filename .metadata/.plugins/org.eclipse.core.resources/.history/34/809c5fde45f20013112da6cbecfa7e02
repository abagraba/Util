package com.mimvista.cloud.organizer.verifier;

import com.mimvista.cloud.organizer.panels.VerifierPanel;
import com.mimvista.cloud.organizer.ui.CloudOrganizerTask;
import com.mimvista.util.Messages;

public class VerifierTask extends CloudOrganizerTask {
	/**
	 * Externalized Strings
	 */
	public static Messages props = Messages.getMessages("Verifier");
	/**
	 * Get title for the task
	 */
	@Override
	public String getTitle() {
		return props.getProperty("VERIFIER_TASK_TITLE");
	}
	/**
	 * Get a unique identifier for the task
	 */
	@Override
	public String getIdentifier() {
		return "verifier";
	}
	/**
	 * Close the task
	 */
	@Override
	public void close() {
		
	}
	/**
	 * Initial controller
	 */
	@Override
	public void startInitialController() {
		setActiveComponent(new VerifierPanel());
	}

}
