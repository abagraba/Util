
public class IHDRChunk extends Chunk {

	public IHDRChunk(int size, byte[] header, byte[] data, int crc) {
		super(size, header, data, crc);
	}
	
	protected String chunkDescription() {
		if (size != 13)
			return "\tInvalid IHDR Chunk";
		int width = size[0] << 24 + size [1] << 16 + size[2] << 8 + size[3];
		return desc;
	}

}
