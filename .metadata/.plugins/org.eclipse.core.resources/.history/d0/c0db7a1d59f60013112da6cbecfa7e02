package com.mimvista.cloud.shared.common;

import java.util.List;

public class S3SeriesURL {

	public final String dicomURL;
	public final List<String> bundleURL;

	public S3SeriesURL(String dicomURL, List<String> bundleURL) {
		this.dicomURL = dicomURL;
		this.bundleURL = bundleURL;
	}

	public int numURLs() {
		return bundleURL.size() + (dicomURL != null ? 1 : 0);
	}
}
