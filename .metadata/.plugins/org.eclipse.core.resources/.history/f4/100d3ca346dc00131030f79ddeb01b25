public class ChunksRGB extends Chunk {

	public static void register() {
		ChunkRegistry.registerChunk("sRGB", ChunksRGB.class);
		System.out.println("sRGB Chunk Registered.");
	}

	public ChunksRGB(int size, byte[] header, byte[] data, int crc) {
		super(size, header, data, crc);
	}

	protected boolean valid() {
		return size == 13;
	}
	
	protected String chunkDescription() {
		if (size != 13)
			return "\tInvalid IHDR Chunk";
		return pad("Rendering Intent:") + data[0] + " pixels\n" + pad("Height:") + height
				+ " pixels\n" + pad("Bit Depth:") + data[8] + " bits\n"
				+ pad("Color Type:") + data[9] + "\n"
				+ pad("Compression Method:") + data[10] + "\n"
				+ pad("Filter Method:") + data[11] + "\n"
				+ pad("Interlace Method:") + data[12];
	}

}
